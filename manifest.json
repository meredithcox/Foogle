{
  "manifest_version": 2,
  "name": "Foogle",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "shrek.gif"
  ]
}